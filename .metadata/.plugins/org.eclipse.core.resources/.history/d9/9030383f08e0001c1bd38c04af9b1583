package com.demos;

public class Test {

	
	public int gcd(int g, int s)
	{
		// g = 20, s = 8
		while(g%s!=0)
		{
			int r = g%s;//4
			g = s;
			s = r;
		}
		return s;
	}
	public int lcm(int g,int s,int gcd) {
		return gcd/(g*s);
	}
	public static void main(String[] args) {
		Test t1 = new Test();
		int hcf = t1.gcd(7, 5);
		System.out.println(t1.lcm(7, 5, hcf));

	}

}
